{
  "name": "penrose-zoom",
  "version": "1.0.0",
  "description": "Penrose P2 tiling deflation with zoom",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint **/*.js | snazzy",
    "dev": "budo index.js:bundle.js --live --open",
    "build": "browserify index.js | uglifyjs -m -c warnings=false > bundle.js"
  },
  "keywords": [],
  "author": "Sean Zellmer <sean@lejeunerenard.com> (http://lejeunerenard.com)",
  "license": "MIT",
  "dependencies": {
    "2d-context": "^1.3.0",
    "canvas-loop": "^1.0.7",
    "object-assign": "^4.1.0",
    "tween.js": "^16.3.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.0.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0-beta.3",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "installify": "^1.0.3",
    "snazzy": "^4.0.1",
    "uglify-js": "^2.7.3"
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "installify",
        {
          "save": true
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "globals": {
      "imgDir": false,
      "Image": false,
      "SoundBank": false,
      "Typekit": false,
      "WebFont": false,
      "XMLHttpRequest": false
    }
  }
}
